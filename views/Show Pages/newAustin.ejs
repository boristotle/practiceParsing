    <div class="container">
      <!-- <h1>Add your home</h1> -->

      <form action="/new" method="POST">
        <div class="col-md-3 col-sm-6 well smartphone"><h2>Basics</h2>


          <p><label for="address">Street Address</label>
          <input type="text" id="address" name="address"></p>

          <p><label for"city">City</label>
            <select id="city" name="city">
              <option value="Austin" selected>Austin</option>
              <option value="Bastrop">Bastrop</option>
              <option value="Bee Cave">Bee Cave</option>
              <option value="Buda">Buda</option>
              <option value="Burnet">Burnet</option>
              <option value="Cedar Park">Cedar Park</option>
              <option value="Driftwood">Driftwood</option>
              <option value="Dripping Springs">Dripping Springs</option>
              <option value="Georgetown">Georgetown</option>
              <option value="Horseshoe Bay">Horseshoe Bay</option>
              <option value="Hutto">Hutto</option>
              <option value="Jonestown">Jonestown</option>
              <option value="Kyle">Kyle</option>
              <option value="Lago Vista">Lago Vista</option>
              <option value="Lakeway">Lakeway</option>
              <option value="Leander">Leander</option>
              <option value="Liberty Hill">Liberty Hill</option>
              <option value="Manor">Manor</option>
              <option value="Marble Falls">Marble Falls</option>
              <option value="Pflugerville">Pflugerville</option>
              <option value="Round Rock">Round Rock</option>
              <option value="Spicewood">Spicewood</option>
              <option value="Taylor">Taylor</option>
              <option value="the Hills">the Hills</option>
              <option value="West Lake Hills">West Lake Hills</option>
              <option value="Wimberley">Wimberley</option>
            </select></p>

          <p><label for="price">Price</label><br>
            <input type="text" id="price" name="price"></p>

          <p><label for="sqft">Square Feet</label>
            <input type="text" id="sqft" name="sqft"></p>

            <p><label for"beds">Bedrooms</label>
            <select id="beds" name="beds">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="6">6+</option>
            </select></p>

          <p><label for"baths">Bathrooms</label>
            <select id="baths" name="baths">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5+</option>
            </select></p>

           <p><label for="yearBuilt">Year Built</label>
          <input type="text" id="yearBuilt" name="yearBuilt"></p>

          <p><label for"garage">Garages</label>
            <select id="garage" name="garage">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3+</option>
            </select></p>

          <p><label for"stories">Stories</label>
            <select id="stories" name="stories">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
            </select></p>

             <p><label for="lotSize">Lot Size (acres)</label>
          <input type="text" id="lotSize" name="lotSize"></p>

           <p><label for="subdivision">Subdivision</label>
              <input type="text" id="subdivision" name="subdivision"></p>

            <h2>Schools</h2>

            <p><label for"schoolDistrict">School District</label>
            <select id="schoolDistrict" name="schoolDistrict">
                <option value="Austin ISD">Austin ISD</option>
                <option value="Leander ISD">Leander ISD</option>
                <option value="Round Rock ISD">Round Rock ISD</option>
            </select></p>

            <p><label for="elemSchool">Elementary School</label><br>
            <input type="text" id="elemSchool" name="elemSchool"></p>

            <p><label for="middleSchool">Middle School</label><br>
            <input type="text" id="middleSchool" name="middleSchool"></p>

            <p><label for="highSchool">High School</label><br>
            <input type="text" id="highSchool" name="highSchool"></p>

        </div>

        <div class="col-md-3 col-sm-6 smartphone"><h2>Amenities</h2>

            <p><legend>Pool&nbsp;&nbsp;&nbsp;
            <selection>Yes&nbsp;<input type="radio" name="pool" value="yes"></selection></legend></p>

            <p><legend>Master Main&nbsp;&nbsp;&nbsp;
            <selection>Yes&nbsp;<input type="radio" name="masterMain" value="yes"></selection></legend></p>

            <p><legend>Fireplace&nbsp;&nbsp;&nbsp;
            <selection>Yes&nbsp;<input type="radio" name="fireplace" value="yes"></selection></legend></p>

            <p><legend>Waterfront&nbsp;&nbsp;&nbsp;
            <selection>Yes&nbsp;<input type="radio" name="waterfront" value="yes"></selection></legend></p>

            <p><legend>Greenbelt&nbsp;&nbsp;&nbsp;
            <selection>Yes&nbsp;<input type="radio" name="greenbelt" value="yes"></selection></legend></p>

             <p><label for"hoaDuration">HOA Duration</label>
            <select id="hoaDuration" name="hoaDuration">
                <option value="none">No HOA</option>
                <option value="monthly">Monthly</option>
                <option value="quarterly">Quarterly</option>
                <option value="semi-annually">Semi-Annually</option>
                <option value="yearly">Yearly</option>
            </select></p>

            <p><label for="hoaFee">HOA Fee</label>
              <input type="text" id="hoaFee" name="hoaFee"></p>
          </div>

         <div class="col-md-3 col-sm-7 well smartphone neighborhood"><h2>Neighborhood</h2>

            <p><legend>Comm Pool
            <selection>Yes&nbsp;<input type="radio" name="communityPool" value="yes"></selection></legend></p>

            <p><legend>Golf Comm
            <selection>Yes&nbsp;<input type="radio" name="golfCommunity" value="yes"></selection></legend></p>

            <p><legend>Comm Lake
            <selection>Yes&nbsp;<input type="radio" name="lake" value="yes"></selection></legend></p>

            <p><legend>Comm Park
            <selection>Yes&nbsp;<input type="radio" name="communityPark" value="yes"></selection></legend></p>

          </div>

         <div class="col-md-3 col-sm-5 smartphone"><h2>Description</h2>
           <p><textarea cols="25" rows="10" name="describe"></textarea>
           </p>


         <h2>Financials</h2>

            <p><label for="taxes">Yearly Taxes</label>
              <input type="text" id="taxes" name="taxes"></p>

            <p><label for="taxRate">Tax Rate</label>
              <input type="text" id="taxRate" name="taxRate"></p>

          <h2>Images</h2>

               <p><label for"image">Upload Images</label>
            <!-- <input type="file" id="image" name="image" class="upload_form"></p> -->

              <p>&nbsp;</p>
              <p><input type="submit" value="Add House"></p>
            </div>


        
      </form>
    </div>
    <script src="/javascripts/javascript.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
  </body>
</html>