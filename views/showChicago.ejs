<div class='cont'>

  <div class='row'>
  <div class="rtl">
          <% for (var i = 1; i <= theListing.photoCount; i++) { %>
        <img class="imageSlideshow2" src="/images/chicagoImages/LargePhoto<%= theListing.MLS %>-<%= i %>.jpeg"></a>
        <% } %>
    </div>
  </div>

    <div class='row'>
        <div class='col-md-12 col-xs-12'>
        <h2 id='h2'>DESCRIPTION</h2>
        <div><%= theListing.remarks %></div>
        </div>
    </div>
    <div class='row'>
      <div class='col-md-6 col-xs-12'>
          <h2 id='h2'>BASICS</h2>
          <div><strong><span class="price"><%= theListing.price %></span></strong></div>
          <div><%= ((new Date - new Date(theListing.listDate.trim())) / 86400000).toFixed(0) %> Days on Market</div>
          <div><%= theListing.beds %> Bed, <%= theListing.fullBaths %> Bath, <%= theListing.appxsqft %> Sq.Ft.</div>
          <div class='add'><%= theListing.streetNumber %> <%= theListing.streetName %> <% if (theListing.unitNum !== 'NA') { %>Unit <%= theListing.unitNum %><% } %> in <%= theListing.city %>, <%= theListing.state %></div>
      </div>
      <div class='col-md-6 col-xs-12'>
        <h2 id='h2'>MORE INFO</h2>
        <div>
          <!-- STORIES -->

          <button id="none" disabled="true"><%= theListing.garage %> Garage</button><% if (theListing.fireplace > 0) { %><button id="none" disabled="true">Fireplace</button><% } else { %><% } %><% if (theListing.basement.indexOf('N') < 0) { %><button id="none" disabled="true">Basement</button><% } else { %><% } %><% if (theListing.otherAmenities.indexOf('C') >= 0) { %><button id="none" disabled="true">Comm Pool</button><% } else { %><% } %><% if (theListing.remarks.indexOf('golf') >= 0 || theListing.remarks.indexOf('Golf') >= 0 || theListing.remarks.indexOf('GOLF') >= 0) { %><button id="none" disabled="true"> Golf Comm</button><% } else { %><% } %><% if (theListing.waterfront === 'Y') { %><button id="none" disabled="true">Waterfront</button><% } else { %><% } %><button id="none" disabled="true">Built <%= theListing.appxyearbuilt %></button>
        </div>
    </div>
  </div>
    <div class='row'>
      <div class='col-md-6 col-xs-12'>
        <h2 id='h2'>SCHOOLS</h2>
        <div>Elem: <%= theListing.elemSchool %></div>
        <div>Middle: <%= theListing.middleSchool %></div>
        <div>High School: <%= theListing.highSchool %></div>
      </div>
      <div class='col-md-6 col-xs-12'>
          <h2 id='h2'>NEIGHBORHOOD</h2>
          <div><%= theListing.subdivision %></div>
      </div>
    </div>
    <div class='row'>
        <div class='col-md-6 col-xs-12'>
        <h2 id='h2'>FINANCIALS</h2>
        <div>Taxes: $<%= theListing.taxes %></div>
        <div>Price/SQFT: $<%= theListing.priceSQFT %></div>
        <div>HOA: $<%= theListing.hoaFee %> Monthly</div>
        <div class='est'> </div>
        <div class='favvbutton'>
        <% if (favs) { %> 
         <% if (favs >= 0) { %>
        <form action='removeFav' method='post' class='REMOVEFAV'><button id='none' class='FAVBTN'>Remove Favorite</button></form><% } else { %>

        <form action='/' method='post' class='FAV'><button id='none' class='FAVBTN'><img src="/images/favorite-icon.png" height="20px" width="20px">Add Favorite</button></form><% } } else { %> 

        <form action='/' method='post' class='FAV'><button id='none' class='FAVDISABLED'><img src="/images/favorite-icon.png" height="20px" width="20px">Add Favorite</button></form><% } %>
      </div>
        </div>

      <div class='col-md-6 col-xs-12'>
        <h2 id='h2'>SHOWING INFO</h2>
        <div>Walker Robinson</div>
         <div><img class='agentImage' src='/images/walker-robinson-chicago.jpg' height='100px' width='100px' /></div>
        <div><a href='tel:7734250881'>773-425-0881</a></div>
        <div><a href='mailto:Walker@MyHomesMatchmaker.com'>Walker@MyHomesMatchmaker.com</a></div>
        <div>RE/MAX Edge</div><br>
        <!-- <a href='/searchResultsChicago/1'><button>Back to Search Results</button></a> -->
      </div>
    </div>
    <div class='row'>
      <div class='LO'>Listing courtesy of <%= theListing.listOffice %></div>
    </div>
     <div class='row'>
        <div id="map"></div>
    </div>
    <div class='col-md-12 col-xs-12'>
      <small>â€œ"This representation is based in whole or in part on data supplied by Midwest Real Estate Data, LLC for the period of inception of this website through present day. Midwest Real Estate Data, LLC does not guarantee nor is it in any way responsible for its accuracy. Data maintained by Midwest Real Estate Data, LLC may not reflect all real estate activity in the market."</small>
    </div>
    <div class='chiIDX'><img src='/images/mred_logo.png'></div>
  

  </div>


<script src='/javascripts/javascript.js'></script>
